{   "model": "bert",
    "pretrained_checkpoint": "bert-base-uncased",
    "optimizer": "AdamP",
    "epoch": 10,
    "warmup_step": 0,
    "save": "./checkpoint/sst2_bert_adamp",
    "seed": 0,
    "optimizer_params": {
